@using TatilSeyahat.Models.Sınıflar
@model List<Blog>
@using TatilSeyahat.App_Code
@{
    Layout = null;
}
@foreach (var x in Model)
{
    <li>
        <div class="col-12 ">
            <div class="col-lg-3"><img src="~/uploads/@x.BlogImage"  style="max-height:50px"/></div>
            <div class="col-lg-8"><a href="/Makale/@seo.AdresDuzenle(x.Baslik)-@x.ID">@x.Baslik</a></div>
            
        </div>
       
    </li>
    <br /><br />
}
